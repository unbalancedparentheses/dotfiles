/* Rofi configuration - Nord theme */

configuration {
    modi: "drun,run,window,ssh";
    show-icons: true;
    icon-theme: "Papirus";
    terminal: "st";
    font: "JetBrainsMono Nerd Font 11";
    drun-display-format: "{name}";
    disable-history: false;
    sidebar-mode: false;
}

@theme "nord"
